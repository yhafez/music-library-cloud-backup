{
	"name": "music-library-cloud-backup",
	"version": "1.0.0",
	"author": {
		"name": "Yahya Hafez",
		"email": "yhafez3@gmail.com",
		"url": "https://www.linkedin.com/in/yahya-hafez/"
	},
	"contributors": [
		{
			"name": "Yahya Hafez",
			"email": "yhafez3@gmail.com",
			"url": "https://www.linkedin.com/in/yahya-hafez/"
		}
	],
	"private": true,
	"description": "An application that allows me to backup my music library to the cloud",
	"bugs": {
		"url": "https://github.com/yhafez/music-library-cloud-backup/issues",
		"email": "yhafez3@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/yhafez/music-library-cloud-backup.git"
	},
	"engines": {
		"pnpm": ">=7.13.6",
		"node": ">=18.17.1"
	},
	"type": "module",
	"exports": "./dist/index.js",
	"typings": "src/vite-env.d.ts",
	"scripts": {
		"start": "vite --port $PORT",
		"dev": "vite",
		"start:backend": "nodemon --inspect server/index.ts",
		"start:dev": "concurrently \"pnpm run dev\" \"pnpm run start:backend\"",
		"build": "vite build",
		"preview": "vite preview",
		"test": "NODE_ENV=test jest",
		"lint": "pnpm lint:format && pnpm lint:fix ",
		"lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier  --config ./.prettierrc --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"type-check": "tsc",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"husky:pre-commit": "sh .husky/pre-commit",
		"husky:pre-push": "sh .husky/pre-push"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.437.0",
		"@vitejs/plugin-react": "^2.2.0",
		"axios": "^1.6.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"multer": "1.4.5-lts.1",
		"music-metadata": "^8.1.4",
		"pg": "^8.11.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sanitize-filename": "^1.6.3",
		"vite": "^3.2.7"
	},
	"devDependencies": {
		"@babel/core": "^7.23.2",
		"@babel/preset-env": "^7.23.2",
		"@babel/preset-react": "^7.22.15",
		"@babel/preset-typescript": "^7.23.2",
		"@commitlint/cli": "^17.8.1",
		"@commitlint/config-conventional": "^17.8.1",
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/builder-vite": "^0.2.7",
		"@storybook/cli": "^6.5.16",
		"@storybook/react": "^6.5.16",
		"@storybook/testing-library": "^0.0.13",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.5.1",
		"@types/express": "^4.17.20",
		"@types/jest": "^28.1.8",
		"@types/multer": "^1.4.9",
		"@types/node": "^20.8.9",
		"@types/pg": "^8.10.7",
		"@types/react": "^18.2.33",
		"@types/react-dom": "^18.2.14",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"babel-loader": "^8.3.0",
		"camelcase": "^6.3.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.33.2",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^13.3.0",
		"nodemon": "^3.0.1",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.4",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^4.9.5"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pnpm husky:pre-commit",
			"commit-msg": "pnpm husky:commit-msg"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,js}": [
			"eslint --config .eslintrc"
		],
		"*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
			"prettier --write"
		]
	},
	"jest": {
		"testEnvironment": "jsdom",
		"setupFilesAfterEnv": [
			"<rootDir>/config/jest/jest-setup.ts"
		],
		"moduleNameMapper": {
			"\\.(css|less)$": "identity-obj-proxy",
			"\\.svg": "<rootDir>/config/jest/__mocks__/svg.js"
		}
	}
}
